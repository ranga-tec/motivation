{
  "UsePostgreSQL": true,
  "ConnectionStrings": {
    "DefaultConnection": "${DATABASE_URL}"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    }
  },
  "FileStorage": {
    "RootPath": "/app/storage",
    "MaxFileSizeMB": 10,
    "AllowedExtensions": [ ".pdf", ".jpg", ".jpeg", ".png", ".docx" ]
  },
  "AllowedHosts": "*"
}
